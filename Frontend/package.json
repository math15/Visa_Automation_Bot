{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "nx exec -- nx exec -- next build",
    "start": "next start",
    "lint": "nx exec -- nx exec -- next lint",
    "lint:fix": "nx exec -- nx exec -- next lint --fix",
    "preview": "next build && next start",
    "typecheck": "nx exec -- nx exec -- tsc --noEmit",
    "format:write": "nx exec -- nx exec -- prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "nx exec -- nx exec -- prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "format:all": "nx exec -- nx exec -- prettier --config .prettierrc --ignore-path .prettierignore --write ."
  },
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.2.6",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.424.0",
    "million": "^3.1.11",
    "next": "^14.2.33",
    "next-themes": "^0.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sharp": "^0.33.4",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@nx/next": "19.5.6",
