    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "nx": "19.5.6",
    "postcss": "^8.4.41",
    "postcss-import": "^16.1.0",
    "tailwindcss": "^3.4.7",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=18"
  },
  "nx": {}
