<script>
    var locationData = [{"Id":"8457a52e-98be-4860-88fc-2ce11b80a75e","Name":"Oran","Code":"ORAN","VisaTypeIds":"[\"ec08e478-17f2-4516-914c-4d9198fd8d1e\",\"5f9080db-049c-4e6e-b03c-7c5e2c5b6037\",\"8b24a19f-c120-4c5c-b924-d42eb3151840\"]","MissionId":"7b831dab-5bed-4f9e-9e13-a301dfce2d77"},{"Id":"0566245a-7ba1-4b5a-b03b-3dd33e051f46","Name":"Algiers","Code":"ALGIERS","VisaTypeIds":"[\"c805c157-7e8f-4932-89cf-d7ab69e1af96\",\"9d8b5d90-d9e6-454a-bd0a-1dd81878f0a9\",\"5f9080db-049c-4e6e-b03c-7c5e2c5b6037\",\"8b24a19f-c120-4c5c-b924-d42eb3151840\"]","MissionId":"ec336bcf-29fe-4d76-90f1-a7ae2d74d78b"}];
    var visaTypeData = [{"Id":"ec08e478-17f2-4516-914c-4d9198fd8d1e","Name":"National Visa","VisaTypeCode":"NATIONAL_VISA","AppointmentSource":null},{"Id":"c805c157-7e8f-4932-89cf-d7ab69e1af96","Name":"Schengen Visa","VisaTypeCode":"SCHENGEN_VISA","AppointmentSource":null},{"Id":"9d8b5d90-d9e6-454a-bd0a-1dd81878f0a9","Name":"Schengen visa ( Estonia)","VisaTypeCode":"SCHENGEN_VISA_ESTONIA","AppointmentSource":"WEB_BLS"},{"Id":"5f9080db-049c-4e6e-b03c-7c5e2c5b6037","Name":"First application / premi√®re demande","VisaTypeCode":"SCHENGEN_VISA","AppointmentSource":"WEB_BLS"},{"Id":"8b24a19f-c120-4c5c-b924-d42eb3151840","Name":"Visa renewal / renouvellement de visa","VisaTypeCode":"SCHENGEN_VISA_EXISTING","AppointmentSource":"WEB_BLS"}];
    var vtf = [];
    function vt_change(e) {
        var dataItem = e.sender.dataItem();
        // if (dataItem.VisaTypeCode === 'SCHENGEN_VISA_EXISTING') {
        //     $('#existingVisatype').modal('show');
        // }
        // if (dataItem.VisaTypeCode === 'SCHENGEN_VISA_CASA_ONE') {
        //     $('#Casa1Visatype').modal('show');
        // }
        // else if (dataItem.VisaTypeCode === 'SCHENGEN_VISA_CASA_TWO') {
        //     $('#Casa2Visatype').modal('show');
        // }
        // else if (dataItem.VisaTypeCode === 'SCHENGEN_VISA_CASA_THREE') {
        //     $('#Casa3Visatype').modal('show');
        // }
    }
    $(document).ready(function () {
        $("#VisaType").kendoDropDownList({
            optionLabel: "--Select--",
            dataTextField: "Name",
            dataValueField: "Id",
            autoBind: true,
            //filter: "contains",
            value: '',
            //change: LoadAppointmentDates,
            change: vt_change,
            dataSource: vtf
        });
        $("#LocationId").kendoDropDownList({
            optionLabel: "--Select--",
            dataTextField: "Name",
            dataValueField: "Id",
            autoBind: true,
            //filter: "contains",
            value: '',
            change: onLocationChange,
            dataSource: locationData
        });

    });

    function onLocationChange() {
        debugger;
        var location = $("#LocationId").data('kendoDropDownList');
        if (location.dataItem().VisaTypeIds != null) {
            vtf = visaTypeData.filter(v => location.dataItem().VisaTypeIds.includes(v.Id));
            $("#VisaType").data("kendoDropDownList").setDataSource(vtf);
        }
        //var loc = $("#LocationId").val();
        //vtf = visaTypeData.filter(v => v.LocationId === loc);
        //$("#VisaType").data("kendoDropDownList").setDataSource(vtf);
    }
    function FillApplication(appId, no, service, statusCode) {

        if (statusCode == "BLS_APPOINTMENT_CANCELED") {
            alert("Appointment is Cancellet, You cannot fill Application");
            return;
        }
        if (no == 1) {
            //var returnurl = window.location.href;
            window.location = '/dza/blsapplication/ManageApplication?serviceId=' + appId;
        }
        else {
            var url = '/dza/blsapplication/FillVisaApplicationList?serviceId=' + appId;
            //var win = GetMainWindow();
            //win.iframeOpenUrl = url;
            //win.OpenWindow({ Title: 'Fill Visa Application', Width: 1000, Height: 700 });
            window.location = url;
            return false;
        }

    }
    function LoadManageApplication(appId, id) {

        var returnurl = window.location.href;
        var url = '/dza/blsapplication/ManageApplication?serviceId=' + appId + "&id=" + id;
        window.location = url;

    }
    var editId;
    function ManageApplicant(id, type, locationId) {
        ShowLoader();
        editId = id;
        var location = $("#LocationId").data('kendoDropDownList');
        location.value(locationId);
        if (location.dataItem().VisaTypeIds != null) {
            vtf = visaTypeData.filter(v => location.dataItem().VisaTypeIds.includes(v.Id));
        }
        var vt = $("#VisaType").data("kendoDropDownList");
        vt.setDataSource(vtf);
        vt.value(type);
        if (id === '' || id === null || id === undefined) {
            $("#LocationId").data("kendoDropDownList").select(0);
        }

        $.ajax({
            url: "/dza/appointmentdata/IsAppointmentExist?id=" + editId,
            success: function (result) {
                HideLoader();
                if (result.success) {
                    $('#editAlert').modal('show');
                    return false;
                }
                else {
                    $('#visaTypeModal').modal('show');
                    return false;
                }
            },
            error: function (err) {
                HideLoader();
                ShowError(err);

            }
        });


    }
    function VisaTypeProceed() {
        var locId = $("#LocationId").val();
        if (locId === null || locId === '' || locId === undefined) {
            alert('Please select your location');
            return false;
        }
        var vtId = $("#VisaType").val();
        if (vtId === null || vtId === '' || vtId === undefined) {
            alert('Please select your visa type');
            return false;
        }
        var win = GetMainWindow();
        win.iframeOpenUrl = '/dza/appointmentdata/ManageApplicant?userId=ab3cc1a2-eabc-444c-8ecd-7d292f78eaf4&id=' + editId + '&visaTypeId=' + vtId + '&locationId=' + locId;
        win.OpenWindow({ Title: "ManageApplicant", Width: 1200, Height: 650 });
        $('#visaTypeModal').modal('hide');
        return false;
    }
    var viewId;
    function ViewApplicant(id, type, locationId) {
        viewId = id;

        // $("#LocationId").data("kendoDropDownList").value(locationId);
        // vtf = visaTypeData.filter(v => v.Id === type);
        // $("#VisaType").data("kendoDropDownList").setDataSource(vtf);
        // $("#VisaType").data("kendoDropDownList").select(1);
        // if (id === '' || id === null || id === undefined) {
        //     $("#LocationId").data("kendoDropDownList").select(0);
        // }
        var win = GetMainWindow();
        win.iframeOpenUrl = '/dza/appointmentdata/ViewApplicant?userId=ab3cc1a2-eabc-444c-8ecd-7d292f78eaf4&id=' + viewId;
        win.OpenWindow({ Title: "ViewApplicant", Width: 1200, Height: 650 });

        return false;

    }
    var deleteId = '';
    function DeleteApplicant(id) {
        deleteId = id;
        $('#confirmDeleteModal').modal('show');
        return false;
    }
   
</script>



